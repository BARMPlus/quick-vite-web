{
  "name": "quick-vite-web",
  "version": "0.1.0",
  "private": true,
  "license": "ISC",
  "author": "chenglin",
  "scripts": {
    "build": "NODE_OPTIONS=--max-old-space-size=8192 vite build",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:css": "stylelint \"src/**/*.less\"",
    "lint:css:fix": "stylelint \"src/**/*.less\" --fix",
    "lint:js": "eslint \"src/**/*.{js,jsx,ts,tsx,cjs,mjs}\"",
    "lint:js:fix": "eslint \"src/**/*.{js,jsx,ts,tsx,cjs,mjs}\" --fix",
    "prepare": "husky install",
    "preview": "vite preview",
    "sort-package": "sort-package-json",
    "start": "NODE_OPTIONS=--max-old-space-size=8192 vite"
  },
  "lint-staged": {
    "*.{cjs,mjs,js,jsx,ts,tsx}": [
      "prettier -w",
      "eslint --cache --fix"
    ],
    "*.{css,less}": "stylelint --fix"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "arrowParens": "always",
    "endOfLine": "lf",
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all",
    "useTabs": false
  },
  "dependencies": {
    "@dian/app-utils": "^0.24.7",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.4.1",
    "@mui/material": "^5.8.2",
    "@rematch/core": "^1.4.0",
    "@rematch/immer": "^1.2.0",
    "axios": "^0.21.0",
    "classnames": "^2.2.6",
    "js-cookie": "^2.2.1",
    "lodash": "^4.17.21",
    "query-string": "^7.1.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-redux": "^7.2.0",
    "react-router-dom": "5.1.2",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-sentry-middleware": "^0.1.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@dian/eslint-config": "~1.8.2",
    "@tsconfig/vite-react": "^1.0.0",
    "@types/history": "4.7.7",
    "@types/lodash": "^4.14.182",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "@types/react-router-dom": "5.1.7",
    "@vitejs/plugin-react": "~1.3.2",
    "autoprefixer": "^10.4.7",
    "c8": "^7.11.3",
    "eslint": "~8.16.0",
    "eslint-config-prettier": "~8.5.0",
    "husky": "^8.0.1",
    "jsdom": "^19.0.0",
    "less": "^4.1.3",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.14",
    "postcss-less": "^6.0.0",
    "prettier": "^2.5.1",
    "sort-package-json": "^1.57.0",
    "stylelint": "^14.9.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^26.0.0",
    "tailwindcss": "^3.1.3",
    "typescript": "^4.7",
    "vite": "^2",
    "vite-tsconfig-paths": "^3.5.0"
  },
  "sentryConfig": {
    "dsn": "",
    "enableUploadSourceMap": true
  }
}
